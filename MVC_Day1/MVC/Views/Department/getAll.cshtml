
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Location</th>
            <th scope="col">MgrName</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var d in Model)
        {
            <tr>
                <th scope="row">@d.Id</th>
                <td>@d.Name</td>
                <td>@d.Location</td>
                <td>@d.MgrName</td>

            </tr>
        }
    </tbody>
</table>