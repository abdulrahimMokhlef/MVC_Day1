
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">SSN</th>
            <th scope="col">Age</th>
            <th scope="col">Address</th>
            <th scope="col">Image</th>
            <th scope="col">E_mail</th>



        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model)
        {
            <tr>
                <th scope="row">@d.Id</th>
                <td>@d.Name</td>
                <td>@d.SSN</td>
                <td>@d.Age</td>
                <td>@d.Address</td>
                <td>@d.Image</td>
                <td>@d.E_mail</td>



            </tr>
        }
    </tbody>
</table>