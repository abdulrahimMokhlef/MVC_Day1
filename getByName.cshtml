@model Department
<div class="card" style="width: 18rem;">
    <img src="..." class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">@Model.DeptId</h5>
        <p class="card-text">@Model.Name</p>
        <p class="card-text">@Model.Manager</p>
        <p class="card-text">@Model.Location</p>
        <p class="card-text">@Model.Branch</p>

        @* <a href="#" class="btn btn-primary">Go somewhere</a> *@
    </div>
</div>